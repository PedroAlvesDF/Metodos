#include <stdio.h>

int main()
{
    char j[100] = {};
    char s[100][100] = {{}, {}};
    double coef = 0;
    int l = 0, c = 0;
    scanf("%^\ns", j);
    for(int i = 0; j[i]!='\0'; i++){
        while(j[i]!=' '||j[i]!='\0'||j[i]!='*'||j[i]!='/'||j[i]!='+'||j[i]!='-'||j[i]!='^'||j[i]!='('||j[i]!=')'||j[i]!='e'){
            s[l][c] = j[i];
            i++;
            c++;
        }
    }
        printf("%s\n", s[l]);
        l++;
    }
    return 0;
}
