#include <stdio.h>

int main()
{
    double x[100], y[100], c1, g1, c2, g2, c3, g3, a[3][4], l[3][4], k;
    int n;
    
    scanf("%d", &n);
    for(int i = 0; i<n; i++){
        scanf("%lf %lf", &x[i], &y[i]);
        a[0][0] = n;
        a[0][1] += x[i];
        a[0][2] += x[i]*x[i];
        a[1][0] = a[0][1];
        a[1][1] += x[i]*x[i];
        a[1][2] += x[i]*x[i]*x[i];
        a[2][0] = a[0][2];
        a[2][1] = a[1][2];
        a[2][2] += x[i]*x[i]*x[i]*x[i];
        a[0][3] += y[i];
        a[1][3] += y[i]*x[i];
        a[2][3] += y[i]*x[i]*x[i];
    }
    
    //Escalonar
    
    for(int diag = 0; diag<3; diag++){
        for(int i = diag; i<3; i++){
            if(a[diag][diag] != 0 && a[i+1][diag] != 0){
                k = a[i+1][diag]/a[diag][diag];
          
            for(int j = 0; j<4; j++){
                a[i+1][j] = a[i+1][j] - k*a[diag][j];
            }
            }
        }    
    }
    
    c3 = a[2][3]/a[2][2];
    c2 = (a[1][3]-a[1][2]*c3)/a[1][1];
    c1 = (a[0][3] - a[0][2]*c3 - a[0][1]*c2)/a[0][0];
    
    printf("y =");
    if(c1!=0){
        printf(" %lf", c1);
    }
    if(c2>0){
        if(c1!=0){
            printf(" +");
        }
        printf(" %lfx", c2);
    }
    else if(c2!=0){
        printf(" %lfx", c2);
    }
    if(c3>0){
        printf(" +", c3);
        if(c3!=0){
            printf(" %lfxÂ²", c3);
        }
    }
    printf("\n");
    

    return 0;
}
