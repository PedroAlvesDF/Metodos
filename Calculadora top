double integ(char j[100], double a, double b){
	int np = 10000;
	double h = (b-a)/np;
	double xk = a, xk1 = a+h;
	double ff=0;
	
	for(int i = 0; i<np; i++){
		ff += f(xk) + 4*f((xk+xk1)/2) + f(xk1);
		xk = xk1;
		xk1 += h;
	}
	ff = (h/6)*ff;
    return ff;    
}

