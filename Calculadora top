#include <stdio.h>
#include <string.h>
#include <math.h>

double f(char j[100]){
    int a = strlen(j);
    double coef = 0;
    int c = 0;
    double z;
    for(int i = 0; i<a; i++){
        if(j[i]>=48&&j[i]<=57){
            c = i;
            while(j[c]>=48&&j[c]<=57){
                c++;
            }
            int b = 0;
            for(;i<c;i++){
                coef = (j[c]-48)*pow(10, c-i);
            }
            printf("%lf", coef);
        }
        switch(j[i]){
            case 'x':
            break;
            case '1':
                z = 1;
            break;
            case '2':
                z = 2;
            break;
            case '3':
                z = 3;
            break;
            case '4':
                z = 4;
            break;
            case '5':
                z = 5;
            break;
            case '6':
                z = 6;
            break;
            case '7':
                z = 7;
            break;
            case '8':
                z = 8;
            break;
            case '9':
                z = 9;
            break;
            case '0':
                z = 0;
            break;
            case '*':
                
            break;
            case '-':
            break;
            case '+':
            break;
            case '/':
            break;
            case ' ':
            break;
            case '.':
            break;
            case '(':
            break;
            case ')':
            break;
        }
    }
    
    return 0;
}

